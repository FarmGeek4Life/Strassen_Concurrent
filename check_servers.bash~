#!/bin/bash

for ((i = 1; i <= 35; i+=1))do
  #echo "---------------------------- aus213l$i ----------------------------------"
  ((echo -e 'for PID in $(ps -eo pid,user,cmd | grep "server_leaf" | grep -v "grep" | grep -E -o -e "^[0-9]{1,7}")\n do\n kill -9 $PID\n echo "killed server_leaf on $(hostname -i)"\n done\n' ; \
  exit) | ssh -T -p 215 aus213l$i) &
  #echo ""
done